{
  "name": "epic-api-service",
  "version": "1.0.0",
  "description": "Epic FHIR API integration service for healthcare federation",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "test": "jest"
  },
  "dependencies": {
    "@apollo/server": "^4.11.0",
    "@apollo/subgraph": "^2.10.0",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.3",
    "@types/node": "^20.8.0",
    "ioredis": "^5.3.2",
    "jest": "^29.0.3",
    "nock": "^13.4.0",
    "nodemon": "^3.1.10",
    "ts-jest": "^29.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": ["src"],
    "testMatch": ["**/__tests__/**/*.test.ts"]
  }
}